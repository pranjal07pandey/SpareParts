<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cardviewdelivered"
    android:clickable="true"
    android:foreground="?android:attr/selectableItemBackground"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:contextClickable="true"
    android:layout_marginTop="2dp"
    cardview:cardBackgroundColor="@color/Titlehome"
    xmlns:cardview="http://schemas.android.com/apk/res-auto"
    cardview:cardCornerRadius="4dp">



<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="25dp"
    android:orientation="horizontal"
    android:id="@+id/linearlatou"
    android:weightSum="5"
    android:background="@drawable/kk"
    >

    <TextView
        android:id="@+id/deliveredname"
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:text="Pranjal"
        android:layout_margin="2dp"
        android:ellipsize="marquee"
        android:maxLength="20"
        android:layout_weight="1"
        android:textAlignment="center"

        android:textColor="@color/background"
        android:textSize="15sp"
        android:textStyle="normal" />

    <TextView
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:id="@+id/deliveredcode"
        android:textSize="15sp"
        android:layout_margin="2dp"
        android:textColor="@color/background"
        android:ellipsize="marquee"
        android:maxLength="10"
        android:layout_weight="1"
        android:textAlignment="center"
        android:text="123"
        android:textStyle="normal"/>

    <TextView
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:id="@+id/deliveredaddress"
        android:layout_margin="2dp"
        android:textSize="15sp"
        android:textColor="@color/background"
        android:ellipsize="marquee"
        android:textAlignment="center"

        android:maxLength="10"
        android:layout_weight="1"
        android:layout_below="@+id/docscategory_title"
        android:text="Nepal"
        android:textStyle="normal"/>


    <TextView
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:id="@+id/deliveredquantity"
        android:layout_margin="2dp"
        android:textSize="15sp"
        android:textColor="@color/background"
        android:ellipsize="marquee"
        android:textAlignment="center"

        android:maxLength="10"
        android:layout_weight="1"
        android:layout_below="@+id/docscategory_title"
        android:text="123"
        android:textStyle="normal"/>


    <TextView
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:id="@+id/deliveredprice"
        android:layout_margin="2dp"
        android:textSize="15sp"
        android:textColor="@color/background"
        android:ellipsize="marquee"
        android:textAlignment="center"

        android:maxLength="10"
        android:layout_weight="1"
        android:layout_below="@+id/docscategory_title"
        android:text="123"
        android:textStyle="normal"/>

